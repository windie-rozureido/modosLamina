Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"coeficienteB", "[", 
    RowBox[{"\[Omega]_", ",", " ", "k_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"-", 
     FractionBox[
      RowBox[{"\[Omega]", "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", "\[Nu]"}], ")"}], 
        SuperscriptBox["k", "2"], 
        RowBox[{"Cosh", "[", 
         FractionBox[
          SqrtBox[
           RowBox[{"\[Omega]", "+", 
            SuperscriptBox["k", "2"]}]], "2"], "]"}]}]}], 
      RowBox[{"\[Omega]", "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", "\[Nu]"}], ")"}], 
        SuperscriptBox["k", "2"], 
        RowBox[{"Cos", "[", 
         FractionBox[
          SqrtBox[
           RowBox[{"\[Omega]", "+", 
            SuperscriptBox["k", "2"]}]], "2"], "]"}]}]}]]}], "*", "a"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"coeficienteBPrima", "[", 
    RowBox[{"\[Omega]_", ",", " ", "k_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"-", 
     FractionBox[
      RowBox[{"\[Omega]", "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", "\[Nu]"}], ")"}], 
        SuperscriptBox["k", "2"], 
        RowBox[{"Sinh", "[", 
         FractionBox[
          SqrtBox[
           RowBox[{"\[Omega]", "+", 
            SuperscriptBox["k", "2"]}]], "2"], "]"}]}]}], 
      RowBox[{"\[Omega]", "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", "\[Nu]"}], ")"}], 
        SuperscriptBox["k", "2"], 
        RowBox[{"Sin", "[", 
         FractionBox[
          SqrtBox[
           RowBox[{"\[Omega]", "+", 
            SuperscriptBox["k", "2"]}]], "2"], "]"}]}]}]]}], "*", 
    "aPrime"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"frecuencia\[Omega]SimPositivo", "[", 
    RowBox[{"waveNumber_", ",", " ", "offset_"}], "]"}], ":=", " ", 
   RowBox[{"Re", "[", 
    RowBox[{"w", "/.", 
     RowBox[{"First", "[", 
      RowBox[{"With", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"k", "=", "waveNumber"}], "}"}], ",", " ", 
        RowBox[{"FindRoot", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"Sqrt", "[", 
              RowBox[{"w", "-", 
               RowBox[{"k", "^", "2"}]}], "]"}], "*", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"w", "+", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"1", "-", "\[Nu]"}], ")"}], 
                 RowBox[{"k", "^", "2"}]}]}], ")"}], "^", "2"}], " ", "*", 
             RowBox[{"Cosh", "[", 
              RowBox[{
               RowBox[{"Sqrt", "[", 
                RowBox[{"w", "+", 
                 RowBox[{"k", "^", "2"}]}], "]"}], "/", "2"}], "]"}], "*", 
             RowBox[{"Sin", "[", 
              RowBox[{
               RowBox[{"Sqrt", "[", 
                RowBox[{"w", "-", 
                 RowBox[{"k", "^", "2"}]}], "]"}], "/", "2"}], "]"}]}], "-", 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"Sqrt", "[", 
              RowBox[{"w", "+", 
               RowBox[{"k", "^", "2"}]}], "]"}], "*", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"w", "-", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"1", "-", "\[Nu]"}], ")"}], 
                 RowBox[{"k", "^", "2"}]}]}], ")"}], "^", "2"}], " ", "*", 
             RowBox[{"Sinh", "[", 
              RowBox[{
               RowBox[{"Sqrt", "[", 
                RowBox[{"w", "+", 
                 RowBox[{"k", "^", "2"}]}], "]"}], "/", "2"}], "]"}], "*", 
             RowBox[{"Cos", "[", 
              RowBox[{
               RowBox[{"Sqrt", "[", 
                RowBox[{"w", "-", 
                 RowBox[{"k", "^", "2"}]}], "]"}], "/", "2"}], "]"}]}]}], "==",
            "0"}], ",", 
          RowBox[{"{", 
           RowBox[{"w", ",", 
            RowBox[{
             RowBox[{
              RowBox[{"Abs", "[", "k", "]"}], "^", "2"}], "+", "offset"}]}], 
           "}"}], ",", " ", 
          RowBox[{"WorkingPrecision", "->", "MachinePrecision"}]}], "]"}]}], 
       "]"}], "]"}]}], "]"}]}], ";"}]}], "Input",
